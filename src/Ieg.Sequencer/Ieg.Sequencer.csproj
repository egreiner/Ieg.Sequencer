<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFramework>netstandard2.0</TargetFramework>
	  <LangVersion>latest</LangVersion>

	  <RepositoryUrl>https://github.com/egreiner/Ieg.Sequencer</RepositoryUrl>
	  <RepositoryEmail>sequencer@ieg-tools.net</RepositoryEmail>
	  <PackageProjectUrl>https://github.com/egreiner/Ieg.Sequencer</PackageProjectUrl>
	  <PackageReadmeFile></PackageReadmeFile>
	  <PackageId>Ieg.Sequencer</PackageId>
