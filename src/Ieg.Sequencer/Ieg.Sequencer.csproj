  <ItemGroup>
    <None Remove="Images\LICENSE.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FluentValidation" Version="11.0.2" />
  </ItemGroup>

  <ItemGroup>
	  <None Include="Images\icon2.png">
		  <Pack>True</Pack>
		  <PackagePath></PackagePath>
	  </None>
