<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>latest</LangVersion>

		<RepositoryUrl>https://github.com/egreiner/Ieg.Sequencer</RepositoryUrl>
		<PackageProjectUrl>https://github.com/egreiner/Ieg.Sequencer</PackageProjectUrl>
		<PackageReadme>https://github.com/egreiner/Ieg.Sequencer/blob/master/README.md</PackageReadme>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<License>https://github.com/egreiner/Ieg.Sequencer/blob/master/LICENSE.txt</License>
		<PackageId>IegTools.Sequencer</PackageId>
		<Title>IegTools.Sequencer</Title>
		<Version>1.6.0</Version>
		<Company>ieg Systems</Company>
		<Authors>Ernst Greiner</Authors>
		<Copyright>(c) 2022 ieg Systems - Ing. Ernst Greiner</Copyright>
		<Description>IegTools.Sequencer allows you to create sequences in code, simple to use and simple to extend. It helps you get rid of long unreadable if/else statements that represent a sequence of events.</Description>
		<PackageTags>sequence;statemachine</PackageTags>
		<PackageReleaseNotes>Add Sequence.HasCurrentState(state)</PackageReleaseNotes>
		<NeutralLanguage>en</NeutralLanguage>
		<RepositoryType>git</RepositoryType>
		<UserSecretsId>940bf605-ef83-43f5-9f75-715291de7535</UserSecretsId>
		<PackageIcon>icon2.png</PackageIcon>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Images\LICENSE.txt" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="FluentValidation" Version="11.1.0" />
	</ItemGroup>

	<ItemGroup>
		<None Include="Images\icon2.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="Documentation/README.md" Pack="true" PackagePath="" />
		<None Include="Documentation/License.txt" Pack="true" PackagePath="" />
	</ItemGroup>

</Project>